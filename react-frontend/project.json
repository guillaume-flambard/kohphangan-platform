{
  "name": "kohphangan-frontend",
  "root": "kohphangan/react-frontend",
  "sourceRoot": "kohphangan/react-frontend/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run build",
        "cwd": "kohphangan/react-frontend"
      },
      "configurations": {
        "production": {
          "command": "npm run build"
        }
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run dev",
        "cwd": "kohphangan/react-frontend"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run preview",
        "cwd": "kohphangan/react-frontend"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run lint",
        "cwd": "kohphangan/react-frontend"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run test",
        "cwd": "kohphangan/react-frontend"
      }
    }
  },
  "tags": ["scope:kohphangan", "type:app", "platform:react"]
}